---
interface Props {
  size?: number;
}
const { size = 1 } = Astro.props;
---

<svg
  height={`${size}em`}
  width={`${size}em`}
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
>
  <line x1="3" x2="21" y1="6" y2="6"></line>
  <line x1="3" x2="21" y1="12" y2="12"></line>
  <line x1="3" x2="21" y1="18" y2="18"></line>
</svg>
